{
    "name": "Jira Modifier",
    "version": "0.1",
    "description": "",
    "permissions": [
        "http://create.atlassian.net/"
    ],
    "manifest_version": 2,
    "content_scripts": [{
        "js": [
            "content.js"
        ],
        "css": [
            "content.css"
        ],
        "run_at": "document_end",
        "matches": [
            "*://create.atlassian.net/secure/*"
        ],
        "all_frames": true
    }]
}
